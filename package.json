{
  "name": "snyk-resolve-deps",
  "description": "Resolves a node package tree with combined support for both npm@2 and npm@3.",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "directories": {
    "test": "test"
  },
  "scripts": {
    "lint": "jscs lib/*.js -v",
    "check-tests": "! grep 'test.only' test/*.test.js -n",
    "env": "node -e 'console.log(process.env, process.versions)'",
    "cover": "tap test/*.test.js --cov --coverage-report=lcov",
    "test": "npm run check-tests && npm run lint && tap test/*.test.js --cov --timeout=60"
  },
  "author": "Remy Sharp",
  "license": "Apache-2.0",
  "devDependencies": {
    "jscs": "^3.0.7",
    "proxyquire": "^1.7.4",
    "sinon": "^1.17.3",
    "snyk-resolve-deps-fixtures": "^1.1.6",
    "tap": "^5.1.1",
    "tap-only": "0.0.5",
    "tape": "^4.4.0"
  },
  "dependencies": {
    "ansicolors": "^0.3.2",
    "debug": "^3.2.5",
    "lodash.clone": "^4.5.0",
    "lodash.get": "^4.4.2",
    "lodash.set": "^4.3.2",
    "lodash.assign": "^4.2.0",
    "lodash.flatten": "^4.4.0",
    "lodash.assignin": "^4.2.0",
    "lru-cache": "^4.0.0",
    "semver": "^5.5.1",
    "snyk-module": "^1.6.0",
    "snyk-resolve": "^1.0.0",
    "snyk-tree": "^1.0.0",
    "snyk-try-require": "^1.1.1",
    "then-fs": "^2.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Snyk/resolve-deps.git"
  }
}
