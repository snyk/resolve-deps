{
  "name": "snyk-resolve-deps",
  "version": "1.0.0",
  "description": "Resolves a node package tree with combined support for both npm@2 and npm@3.",
  "main": "lib/index.js",
  "directories": {
    "test": "test"
  },
  "bin": {
    "snyk-resolve": "./cli/index.js"
  },
  "scripts": {
    "lint": "jscs cli/*.js cli/**/*.js lib/*.js -v",
    "test": "npm run lint && tap test/*.test.js --cov --timeout=60 && snyk test > /dev/null"
  },
  "author": "Remy Sharp",
  "license": "Apache-2.0",
  "devDependencies": {
    "jscs": "^2.8.0",
    "tap": "^5.1.1",
    "tape": "^4.4.0"
  },
  "dependencies": {
    "@remy/npm-tree": "^1.0.1",
    "ansicolors": "^0.3.2",
    "debug": "^2.2.0",
    "es6-promise": "^3.0.2",
    "lodash": "^4.0.0",
    "lru-cache": "^4.0.0",
    "semver": "^5.1.0",
    "snyk": "*",
    "then-fs": "^2.0.0"
  }
}
